{
  "name": "world-warm-worm/autodoc",
  "description": "Generates documentation of endpoint methods in several file formats",
  "type": "library",
  "homepage": "https://github.com/worldWarmWorm/api-autodoc",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "v.davydkin",
      "email": "world-warm-worm@ya.ru"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "8.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "phpstan/phpstan": "2.1.x-dev",
    "phpstan/phpstan-doctrine": "2.0.x-dev",
    "phpstan/phpstan-strict-rules": "2.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Autodoc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Autodoc\\Tests\\": "tests/"
    }
  }
}
