{
  "name": "world-warm-worm/api-autodoc",
  "description": "Generates documentation of api endpoints in several file formates",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "v.davydkin",
      "email": "st.valeriq@gamil.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "8.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "phpstan/phpstan": "2.1.x-dev",
    "phpstan/phpstan-doctrine": "2.0.x-dev",
    "phpstan/phpstan-strict-rules": "2.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "ApiAutodoc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiAutodoc\\Tests\\": "tests/"
    }
  }

}
